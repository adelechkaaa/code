def pref(w):
    p = (len(w)+1) * [0]
    for i in range(1, len(p)):
        p[i] = p[i-1] + w[i-1]
    return p

n = int(input())

a = list(map(int, input().split()))
a.sort()
#n = len(a)
#print(a)
if n % 2 == 0:
    l = (n // 2) - 1
    r = (n // 2)
else:
    l = (n // 2) - 1
    r = (n // 2) + 1
    print(a[n // 2], end=' ')

for i in range(len(a)//2):
    if a[r] > a[l]:
        print(a[l], a[r], end=' ')
    else:
        print(a[r], a[l], end=' ')
    l -= 1
    r += 1
